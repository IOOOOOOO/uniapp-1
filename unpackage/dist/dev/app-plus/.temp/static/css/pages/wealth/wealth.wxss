
page {
  background: #FFFFFF;
}
.search-color {
  width: 100%;
  display: flex;
  overflow-x: hidden;
  flex-direction: row;
  background: #8cafea;
  height: 70rpx;
}
.search-img {
  width: 40rpx;
  height: 40rpx;
  padding: 15rpx 30rpx;
}
.search-left {
  width: 100rpx;
  height: 100rpx;
}
.search {
  width: 530rpx;
  height: 40rpx;
  margin: 15rpx 10rpx;
  text-align: center;
  line-height: 40rpx;
  color: #FFFFFF;
}
.search-right {
  width: 100rpx;
  height: 100rpx;
}
.persion,
.persion-coin,
.persion-manage,
.list,
.wealth-bottom {
  width: 100%;
}
.persion {
  height: 370rpx;
  display: flex;
  flex-direction: column;
  background: #8cafea;
}
.persion-coin {
  height: 100rpx;
  text-align: center;
  display: block;
  line-height: 100rpx;
  color: #FFFFFF;
}
.persion-manage {
  display: flex;
  flex-direction: row;
  height: 170rpx;
}
.persion-manage-img {
  width: 60rpx;
  height: 60rpx;
  margin-left: 20rpx;
}
.text {
  height: 50rpx;
  line-height: 50rpx;
}
.persion-manage-sub {
  width: 125rpx;
  font-size: 25rpx;
  color: #FFFFFF;
  margin: 28rpx 65rpx;
}
.list {
  border-bottom: 2rpx solid #f2f2f2;
  height: 145rpx;
}
.list-sub {
  width: 700rpx;
  display: flex;
  flex-direction: row;
  margin: 0 25rpx;
}
.list-left {
  width: 62rpx;
  height: 62rpx;
  margin-top: 40rpx;
}
.list-center {
  flex: 1;
  margin-left: 35rpx;
  margin-top: 35rpx;
}
.list-right {
  width: 20rpx;
  height: 29rpx;
  margin-top: 58rpx;
}
.list-title,
.list-des {
  height: 30rpx;
  line-height: 30rpx;
  color: #6a6a6a;
}
.list-title {
  font-weight: bold;
}
.list-des {
  margin-top: 10rpx;
}
.renwu {
  width: 320rpx;
  height: 136rpx;
  background: #8cafea;
}