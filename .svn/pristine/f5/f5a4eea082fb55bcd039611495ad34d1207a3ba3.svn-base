
.page {
  background: #FFFFFF;
  width: 100%;
  height: 100vh;
}
.header {
  height: 100rpx;
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  background: #FFFFFF;
}
.header-left,
.header-right {
  width: 100rpx;
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}
.header image {
  width: 40rpx;
  height: 40rpx;
}
.header-content {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  text-align: center;
}
.uni-list-cell-pd {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 200rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  margin-bottom: 60rpx;
  padding-right: 40rpx;
  box-sizing: border-box;
}
.list-item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  height: 200rpx;
  border: 2rpx solid #EEEEEE;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  overflow: hidden;
}
.carimg {
  height: 200rpx;
  width: 200rpx;
  box-sizing: border-box;
  padding: 20rpx;
}
.carimg image {
  width: 100%;
  height: 100%;
}
.carRight {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  height: 200rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 30rpx 0;
}
.car-price {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 80rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}
.fix {
  width: 750rpx;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #FFFFFF;
  border: 2rpx solid #EEEEEE;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}
.fix ._label {
  font-size: 20rpx;
  color: #b6b6b6;
}
.fixLeft {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.fixright {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.fixnum {
  line-height: 100rpx;
}
.fixnum ._span {
  color: #e16465;
}
.fixbtn {
  margin-left: 20rpx;
  width: 140rpx;
  height: 100rpx;
  background-color: #e97b78;
  line-height: 100rpx;
  color: #FFFFFF;
  text-align: center;
}
.uni-numbox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
  height: 50rpx;
  box-sizing: border-box;
  border-radius: 6rpx;
  margin-right: 40rpx;
  box-sizing: border-box;
  overflow: hidden;
  border-top: 2rpx solid #cccccc;
  border-bottom: 2rpx solid #cccccc;
}
.uni-numbox-minus,
.uni-numbox-plus {
  margin: 0;
  background-color: #f9f9f9;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #555555;
}
.uni-numbox-minus {
  border-left: 2rpx solid #cccccc;
  border-right: none;
}
.uni-numbox-plus {
  border-right: 2rpx solid #cccccc;
  border-left: none;
}
.uni-numbox-value {
  border-left: 2rpx solid #cccccc;
  border-right: 2rpx solid #cccccc;
  background-color: #ffffff;
  width: 60rpx;
  height: 50rpx;
  font-size: 28rpx;
  text-align: center;
}
.uni-numbox-disabled {
  color: #c0c0c0;
}