
.header-fix {
  height: 100rpx;
  width: 100%;
}
.header {
  height: 100rpx;
  width: 100%;
  display: flex;
  align-items: center;
  background: #8cafea;
  position: fixed;
}
.header-left,
.header-right {
  width: 100rpx;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.header image {
  width: 40rpx;
  height: 40rpx;
}
.header-content {
  flex: 1;
  text-align: center;
}
.search-box {
  width: 100%;
  height: 120rpx;
  background: #f7f7f7;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search {
  width: 600rpx;
  height: 70rpx;
  background-color: #FFFFFF;
  border-radius: 36rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #b6b6b6;
}
.search-icon {
  max-width: 36rpx;
  max-height: 36rpx;
  margin-right: 10rpx;
}
.friend-li {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
}
.friend-li image {
  width: 40rpx;
  height: 40rpx;
}
.border-bot {
  border-bottom: 2rpx solid #f7f7f7;
}
.mglr {
  margin: 0 20rpx;
}
.Addressbook-title {
  height: 60rpx;
  width: 100%;
  background: #f7f7f7;
  box-sizing: border-box;
  padding-left: 20rpx;
  display: flex;
  align-items: center;
}
.Addressbook-item {
  width: 100%;
  box-sizing: border-box;
  height: 160rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
  border-bottom: 4rpx solid #f7f7f7;
}
.Addressbook-item-img image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
}
.list {
  position: fixed;
  top: 158rpx;
  right: 10rpx;
  bottom: 0;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 40rpx;
  z-index: 1;
}
.item {
  line-height: 40rpx;
  text-align: center;
  color: #888;
}