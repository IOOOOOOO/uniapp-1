
page {
  background: #FFFFFF;
}
.swiper-item {
  display: block;
  height: 370rpx;
  text-align: center;
}
.search-color {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  overflow-x: hidden;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  background: #1E1E78;
  height: 70rpx;
}
.search-img {
  width: 40rpx;
  height: 40rpx;
  padding: 15rpx 30rpx;
}
.swiper-img {
  width: 100%;
}
.search-left {
  width: 100rpx;
  height: 100rpx;
}
.search {
  width: 530rpx;
  height: 40rpx;
  margin: 15rpx 10rpx;
  background: #FFFFFF;
  border-radius: 200rpx;
}
.search-right {
  width: 100rpx;
  height: 100rpx;
}
.category,
.tuijian-title,
.tuijian,
.tuijian-des,
.tuijian-img,
.tuijian-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 680rpx;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
}
.tuijian-img {
  width: 100%;
  height: 278rpx;
  margin-top: 10rpx;
}
.category {
  height: 175rpx;
  margin: 20rpx 35rpx 0 35rpx;
}
.tuijian-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  height: 90rpx;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  margin-top: 20rpx;
}
.price {
  color: #e97b78;
}
.tuijian-title {
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-weight: bold;
  display: block;
  margin: 20rpx 35rpx;
}
.tuijian {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  margin: 10rpx 35rpx 20rpx 35rpx;
}
.tuijian-des {
  height: 40rpx;
  font-size: 25rpx;
  color: #b6b6b6;
  text-align: center;
  display: block;
}
.category-image {
  width: 100rpx;
  height: 100rpx;
}
.category-sub {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.tuijian-content-left {
  width: 400rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  margin-left: 60rpx;
}
.tuijian-content-right {
  width: 180rpx;
}
.tuijian-button {
  width: 155rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 20rpx;
  background: #8cafea;
  color: #FFFFFF;
}
.text {
  font-size: 25rpx;
  color: #7d7d7d;
}
.ml-40 {
  margin-left: 40rpx;
}
.ml-65 {
  margin-left: 65rpx;
}