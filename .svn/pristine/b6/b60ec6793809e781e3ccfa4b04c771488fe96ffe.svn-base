
.header {
  height: 100rpx;
  width: 100%;
  display: flex;
  align-items: center;
  background: #FFFFFF;
}
.header-left,
.header-right {
  width: 100rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.header image {
  width: 40rpx;
  height: 40rpx;
}
.header-content {
  flex: 1;
  text-align: center;
}
.content {
  display: flex;
  align-items: center;
  flex-direction: column;
}
.phone-img {
  margin: 40rpx 0;
  max-width: 320rpx;
  max-height: 320rpx;
}
.phone-img image {
  max-width: 320rpx;
  max-height: 320rpx;
}
.tel {
  font-size: 36rpx;
  color: #585858;
  margin-bottom: 40rpx;
}
.btn {
  width: 400rpx;
  height: 70rpx;
  border-radius: 18rpx;
  text-align: center;
  line-height: 70rpx;
  margin-top: 40rpx;
  background: #fff;
  color: #b6b6b6;
}
.content .green {
  background: #79c343;
  color: #fff;
}